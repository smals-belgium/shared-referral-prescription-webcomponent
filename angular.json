{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wc-prescription-details": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "wc-prescription-details",
      "sourceRoot": "wc-prescription-details/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/wc-prescription-details",
            "index": "wc-prescription-details/src/index.html",
            "browser": "wc-prescription-details/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "wc-prescription-details/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "./dist/evf-form-details/build/",
                "output": "./assets/evf-form-details/"
              },
              {
                "glob": "**/*",
                "input": "./dist/pdfmake/build/",
                "output": "./assets/pdfmake/"
              },
              {
                "glob": "keycloak.js",
                "input": "./node_modules/keycloak-js/dist/",
                "output": "./"
              }
            ],
            "styles": [
              "wc-prescription-details/src/styles.scss"
            ],
            "scripts": [
              "node_modules/marked/marked.min.js"
            ],
            "externalDependencies": [
              "/assets/evf-form-details/evf-form-details.js",
              "/assets/pdfmake/pdfmake.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "3mb"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "fonts": false
              }
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "wc-prescription-details:build:production"
            },
            "development": {
              "buildTarget": "wc-prescription-details:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
