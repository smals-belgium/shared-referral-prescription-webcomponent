{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wc-prescription-details": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "wc-prescription-details",
      "sourceRoot": "wc-prescription-details/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/wc-prescription-details",
            "index": "wc-prescription-details/src/index.html",
            "browser": "wc-prescription-details/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "wc-prescription-details/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "./dist/evf-form-details/build/",
                "output": "./assets/evf-form-details/"
              },
              {
                "glob": "**/*",
                "input": "./dist/pdfmake/build/",
                "output": "./assets/pdfmake/"
              }
            ],
            "styles": [
              "wc-prescription-details/src/styles.scss"
            ],
            "scripts": [
              "node_modules/marked/marked.min.js"
            ],
            "externalDependencies": [
              "/assets/evf-form-details/evf-form-details.js",
              "/assets/pdfmake/pdfmake.js"
            ],
            "allowedCommonJsDependencies": [
              "localforage",
              "buffer",
              "@messageformat/core",
              "elliptic",
              "bn.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "3mb"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "fonts": false
              }
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "wc-prescription-details:build:production"
            },
            "development": {
              "buildTarget": "wc-prescription-details:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "configPath": "jest.config.ts",
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": [
              "scss"
            ],
            "assets": [],
            "styles": [],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "wc-prescription-details/src/**/*.ts",
              "wc-prescription-details/src/**/*.html"
            ]
          }
        }
      }
    },
